  "scripts": {
    "ng": "ng",
    "start": "node server.ts",
    "build": "npm run build:ssr",
    "staging": "node ./node_modules/@angular/cli/bin/ng build --no-progress",
    "prod": "node ./node_modules/@angular/cli/bin/ng build --no-progress",
    "watch": "ng build --watch --configuration staging",
    "test": "ng test",
    "dev:ssr": "ng run lama-web-frontend:serve-ssr",
    "serve:ssr": "node dist/lama-web-frontend/server/main.js",
    "build:ssr": "ng build && ng run lama-web-frontend:server",
    "prerender": "ng run lama-web-frontend:prerender",
    "lint": "ng lint",
    "build-serve": "export NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng serve",
    "heroku-postbuild": "ng build --configuration development",
    "build-prod": "ng build --configuration=production",
    "build-staging": "ng build --configuration=staging"
  },
  "private": true,
